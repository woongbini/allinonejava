<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	 <bean id="edit" class="org.springframework.web.servlet.view.JstlView">
	 	<property name="url" value="/WEB-INF/views/product/edit.jsp"/>
	 </bean>
	 <bean id="list" class="org.springframework.web.servlet.view.JstlView">
	 	<property name="url" value="/WEB-INF/views/product/list.jsp"/>
	 </bean>
	 <bean id="result" class="org.springframework.web.servlet.view.JstlView">
	 	<property name="url" value="/WEB-INF/views/product/result.jsp"/>
	 </bean>	
</beans>
